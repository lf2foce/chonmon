<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Review</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.content %></td>
        <td><%= link_to 'Show', post %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>