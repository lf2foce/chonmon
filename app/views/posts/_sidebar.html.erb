<div class="col-lg-4 col-md-4">
          <div class="card mb-2" style="border-radius: 6px; background-color: #febb02;">
            <div class="card-body">
              <h5> pg Search </h5>
                <%= form_tag posts_path, class: "", remote: true, method: :get do %>
                  <div class="form-group">
                  <%= text_field_tag "pg_search", params[:pg_search], placeholder: "Enter dish's name ", class: "form-control" %>
                  </div>
                  <%= submit_tag "Search", class: "btn btn-primary" %>
                <% end %>

            </div>
          </div>

          <div class="card" style="border-radius: 6px; border-color: rgb(230, 236, 240);">
            <div class="card-header bg-transparent" style="color: #17bf63">
              <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="mr-1" role="img" viewBox="0 0 24 24" focusable="false"><title>Search</title><circle cx="10.5" cy="10.5" r="7.5"/><path d="M21 21l-5.2-5.2"/></svg>
              <strong>Search filter</strong>
            </div>
            <div class="card-body">
              

              <h5 class="card-title">Categories</h5>

              <%= form_tag posts_path, class: "form-check-test", method: :get, remote: true do %>
                <div class="custom-control custom-checkbox">
                  <%= check_box_tag 'category[]', "1", false, id: "customCheck4", class: "custom-control-input"  %>
                  <label class="custom-control-label" for="customCheck4">Food</label>
                </div>
                <div class="custom-control custom-checkbox">
                  <%= check_box_tag 'category[]', "2", false, id: "customCheck5", class: "custom-control-input" %>
                  <label class="custom-control-label" for="customCheck5">Drink</label>
                </div>
                <div class="custom-control custom-checkbox">
                  <%= check_box_tag 'category[]', "3", false, id: "customCheck6", class: "custom-control-input" %>
                  <label class="custom-control-label" for="customCheck6">Coffee</label>
                </div>
                <%#= submit_tag "Search", class: "btn btn-warning my-2 my-sm-0" %>
                <%= link_to "Reset", posts_path  %>  
              <% end %>

              <%= simple_form_for :filter, url: posts_path, class: "form-check-test2", method: :get, remote: true, defaults: { input_html: { class: 'custom-control-input' } } do |f| %>
                
                  <%= f.input :types, label: "Restaurant",  collection: $types, as: :check_boxes, item_label_class: 'custom-control-label', wrapper_html: { class: 'custom-checkbox' }  %>
                             
                  <%= f.input :interests, label: "Food interests",  collection: $interests, as: :check_boxes, item_label_class: 'custom-control-label', wrapper_html: { class: 'custom-checkbox' }  %>
                  <%= f.input :monan, label: "Kind",  collection: $monan, as: :check_boxes, item_label_class: 'custom-control-label', wrapper_html: { class: 'custom-checkbox' } %>
                <%= submit_tag "Search", class: "btn btn-warning my-2 my-sm-0" %>
                <%= link_to "Reset", posts_path  %>  
              <% end %>

              
              <h5 class="card-title mt-2">Location</h5>


              <div class="custom-control custom-radio">
                <input type="radio" id="customRadio1" name="customRadio" class="custom-control-input">
                <label class="custom-control-label" for="customRadio1">Anywhere</label>
              </div>
              <div class="custom-control custom-radio">
                <input type="radio" id="customRadio2" name="customRadio" class="custom-control-input">
                <label class="custom-control-label" for="customRadio2">Near you</label>
              </div>


              <form>
                <label for="customRange1" class="mt-2">Price range</label>
                <input type="range" class="custom-range" id="customRange1">

   
              </form>
              <h5> Popular Tags </h5>

              <%# @tags.each do |tag| %>
                <%#= link_to "##{tag}", tag_path(:basic => "#{tag}") %>
              <%# end %>

             
            </div>


            <div class="card-footer bg-transparent">Advance search</div>
          </div>



          <div class="mt-3">
            <h5 class="text-muted">
              <div class="">
                <a class="text-muted  " href="#">
                  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="mx-3" role="img" viewBox="0 0 24 24" focusable="false"><title>Search</title><circle cx="10.5" cy="10.5" r="7.5"/><path d="M21 21l-5.2-5.2"/></svg>
                </a>
                Categories
              </div>
            </h5>
            


            <%#= form_tag posts_path, class: "form-inline my-2 my-lg-0 pb-2", method: :get do %>
              <%#= text_field_tag :d, params[:d], placeholder: "Gần đây", class: "form-control mr-sm-2" %>
              <%#= submit_tag "Search", class: "btn btn-warning my-2 my-sm-0" %>
            <%# end %>


            <%#= form_tag posts_path, class: "form-inline my-2 my-lg-0 pb-2", method: :get do %>
              <%#= text_field_tag :search, params[:search], placeholder: "Tìm kiếm", class: "form-control mr-sm-2" %>
              <%#= submit_tag "Search", class: "btn btn-warning my-2 my-sm-0" %>
            <%# end %>


  


            <div class="">
              <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">

                <a class="nav-link" id="v-pills-home-tab" data-toggle="pill" href="#top_choice" role="tab" aria-controls="v-pills-home" aria-selected="true">
                  <%= image_tag("icons/award", size: "20x20") + " Editor choice"  %>

                </a>
                <a class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#home" role="tab" aria-controls="v-pills-home" aria-selected="true">
                  <%= image_tag("icons/radar", size: "20x20") + " Nearby me"  %>
                  <i class="fal fa-coffee fa-fw" style="font-size:1rem;"></i> 
                </a>
                <a class="nav-link" id="v-pills-profile-tab" data-toggle="pill" href="#menu2" role="tab" aria-controls="v-pills-profile" aria-selected="false">
                  <%= image_tag("icons/star", size: "20x20") + " Highest rating"  %>
                  </a>

                <a class="nav-link grab" id="v-pills-messages-tab" data-toggle="pill" href="#menu1" role="tab" aria-controls="v-pills-messages" aria-selected="false">
                  <%= image_tag("icons/fire", size: "20x20") + " Hot "  %>  
                </a>

              </div>
            </div> <!-- end white -->
          
          </div>
        </div>